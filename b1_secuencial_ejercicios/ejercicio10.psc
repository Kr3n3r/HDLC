Proceso B1E10
	Definir x1,y1,x2,y2,distancia Como Entero;
	Escribir 'Escribe la coordenada x del punto 1';
	Leer x1;
	Escribir 'Escribe la coordenada y del punto 1';
	Leer y1;
	Escribir 'Escribe la coordenada x del punto 2';
	Leer x2;
	Escribir 'Escribe la coordenada y del punto 2';
	Leer y2;
	distancia<-raiz((x2-x1)^2+(y2-y1)^2);
	Escribir 'La distancia entre los dos puntos es ', distancia;
FinProceso
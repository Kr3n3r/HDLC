Proceso B1E2
	Definir n, m como real;
	Escribir 'Introduce el valor del primer número';
	Leer n;
	Escribir 'Introduce el valor del segundo número';
	Leer m;
	Escribir 'n+m=', n+m;
	Escribir 'n-m=', n-m;
	Escribir 'm-n=', m-n;
	Escribir 'n*m=', n*m;
	Escribir 'n/m=', n/m;
	Escribir 'm/n=', m/n;
FinProceso
Proceso B2E1
	Definir a,b Como Real;
	Definir BOOLEANO Como Logico;
	
	Escribir 'Introduce el primer valor';
	Leer a;
	Escribir 'Introduce el segundo valor';
	Leer b;
	BOOLEANO<-a>b;
	si BOOLEANO=Verdadero Entonces
		Escribir 'El primer número es mayor que el segundo';
	SiNo
		Escribir 'El primer número no es mayor que el segundo';
	FinSi
FinProceso